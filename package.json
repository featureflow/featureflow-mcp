{
  "name": "featureflow-mcp",
  "version": "1.0.1",
  "description": "MCP (Model Context Protocol) server for Featureflow feature flag management. Enables AI assistants to manage feature flags, projects, environments, and more.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "featureflow-mcp": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "featureflow",
    "feature-flags",
    "feature-toggles",
    "ai",
    "claude",
    "cursor",
    "llm"
  ],
  "author": "Featureflow <support@featureflow.io> (https://featureflow.io)",
  "license": "MIT",
  "homepage": "https://github.com/featureflow/featureflow-mcp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/featureflow/featureflow-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/featureflow/featureflow-mcp/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
